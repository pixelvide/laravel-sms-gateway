        "laravel/framework": "^7.0",
        "aws/aws-sdk-php": "^3.184",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^5"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Nitin Goyal",
            "email": "nitingoyal.dev@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
